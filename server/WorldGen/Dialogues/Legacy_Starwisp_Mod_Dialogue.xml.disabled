<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<dialogues>
	<dialogue name="Vatish">
		<start>M0</start>
		<message id="M0" text="[ACTION]You see a human figure working on the reactor.">
		<!-- Intro conversation -->
			<reply id="R1" text="[SKIP]..." next="M1">
				<condition type="SERVER_VARIABLE_ABSENT" var_name="WISPSHIP_SPAWNED" var_value="1" />
				<condition type="SERVER_VARIABLE_PRESENT" var_name="HJ_TUTORIAL_STARTED" var_value="1" />
			</reply>
			<reply id="R2" text="[SKIP]..." next="TutorialWait">
			<!-- Wait to complete the tutorial -->
				<condition type="SERVER_VARIABLE_ABSENT" var_name="HJ_TUTORIAL_STARTED" var_value="1" />
			</reply>
			<reply id="R3" text="[SKIP]..." next="MPost1">
			<!-- Finished the first conversation. Go talk to Solaria. -->
				<condition type="SERVER_VARIABLE_PRESENT" var_name="WISPSHIP_SPAWNED" var_value="1" />
				<condition type="SERVER_VARIABLE_ABSENT" var_name="SOLARIA_CONVO_1" var_value="1" />
			</reply>
			<reply id="R4" text="Hello? (Ask for a ship)" next="PUCC1">
			<!-- Late players joining conversation aka P.U.C.C -->
				<condition type="SECTOR_SHIPS_ABSENT" ship_name="VAR(CAMPAIGN_SHIP_ID)" qty="1" />
				<condition type="SECTOR_SHIPS_ABSENT" ship_name="P.U.C.C" qty="1" />
				<condition type="SERVER_VARIABLE_PRESENT" var_name="PUCC_BEACON_SECTOR_LOC" var_value="1" any_value="1" />
			</reply>
			<reply id="R5" text="I'm ready. Send the P.U.C.C to the ship!" next="WARP_PUCC1">
			<!-- P.U.C.C ready to go. Waiting for player to get in. -->
				<condition type="SECTOR_SHIPS_PRESENT" ship_name="P.U.C.C" qty="1" />
				<condition type="SECTOR_SHIPS_ABSENT" ship_name="VAR(CAMPAIGN_SHIP_ID)" qty="1" />
				<condition type="SERVER_VARIABLE_PRESENT" var_name="PUCC_BEACON_SECTOR_LOC" var_value="1" any_value="1" />
				<condition type="SHIP_SYSTEM_ABSENT" system_type="HYPERDRIVE" system_model="PUCC_HyperDrive" target_player="1" all_mob_ships="0" active_system="0" qty="1" />
			</reply>
			<reply id="R6" text="[SKIP](Beacon Lost)" next="BeaconLost1">
			<!-- Beacon on the campaign ship is lost -->
				<condition type="SERVER_VARIABLE_PRESENT" var_name="WISPSHIP_SPAWNED" var_value="1" />
				<condition type="SERVER_VARIABLE_ABSENT" var_name="PUCC_BEACON_SECTOR_LOC" var_value="1" any_value="1" />
				<condition type="SERVER_VARIABLE_PRESENT" var_name="CAMPAIGN_SHIP_ID" var_value="1" any_value="1" />
			</reply>
			<reply id="R7" text="[SKIP]..." next="NothingToSay">
			<!-- After initial conversation and spoke to Solaria. Campaign ship hasn't left. Nothing left to do with Vatish. -->
				<condition type="SERVER_VARIABLE_PRESENT" var_name="WISPSHIP_SPAWNED" var_value="1" />
				<condition type="SECTOR_SHIPS_PRESENT" ship_name="VAR(CAMPAIGN_SHIP_ID)" qty="1" />
			</reply>
		</message>
		<message id="TutorialWait" text="[ACTION]They seem to be busy digging around inside. Perhaps you should talk to the captain first.">
		</message>
		<message id="MPost1" text="Well, go on. Get to your ship, contact Solaria and get on your way.">
		</message>
		<message id="NothingToSay" text="[ACTION]It doesn't seem like Vatish has anything to say to you right now.">
		</message>

		<!-- Begin initial conversation -->
		<message id="M1" text="[ACTION]On closer observation, the reactor has been repaired with odd-looking crystals.">
			<reply id="R1" text="[SKIP]..." next="M2" />
		</message>
		<message id="M2" text="[ACTION]Noticing you, the figure quickly withdraws from the reactor.">
			<reply id="R1" text="[SKIP]..." next="M3" />
		</message>
		<message id="M3" text="[ACTION]The figure looks androgynous, short, and rather slim.">
			<reply id="R1" text="[SKIP]..." next="M4" />
		</message>
		<message id="M4" text="[ACTION]Something feels a bit off about their appearance.">
			<reply id="R1" text="Who are you?" next="M5" />
			<reply id="R2" text="You're that Starwisp I saw before I died. (Skip Dialogue)" next="SkipFirstDialogue" />
		</message>
		<!-- Branch for Skip First Dialogue -->
		<message id="SkipFirstDialogue" text="That's right, we agreed to assist you. I'll call our ship.">
			<reply id="R1" text="[SKIP]..." next="SkipFD1" />
		</message>
		<message id="SkipFD1" text="[ACTION]Vatish's form glows momentarily, sending an energy pulse to the reactor..">
			<reply id="R1" text="[SKIP]..." next="M27" />
		</message>
		<!-- Branch if you continue first dialogue -->
		<message id="M5" text="The name's [NPC_NAME]. Are you one of the crew?">
			<reply id="R1" text="Yes, I'm a guard, my name is [PLAYER_NAME]." next="M6" />
		</message>
		<message id="M6" text="I see! The clone replication system must have actually worked!">
			<reply id="R1" text="[SKIP]..." next="M7" />
		</message>
		<message id="M7" text="It was pretty smashed up. But it's okay. I hotwired it back together!">
			<reply id="R1" text="Yes, except that I don't remember anything." next="M8" />
		</message>
		<message id="M8" text="There were some broken pieces left over... I did what I could.">
			<reply id="R1" text="[SKIP]..." next="M9" />
		</message>
		<message id="M9" text="You look fine otherwise... I think.">
			<reply id="R1" text="You aren't part of the crew, are you?" next="M10" />
		</message>
		<message id="M10" text="No. I came on board as soon as the pirates left and did what I could.">
			<reply id="R1" text="Why were you out here in the first place?" next="M11" />
		</message>
		<message id="M11" text="I'm aware of your prototype ship, the DF.">
			<reply id="R1" text="[SKIP]..." next="M12" />
		</message>
		<message id="M12" text="I was sent to investigate it, but the pirates got here first.">
			<reply id="R1" text="What do you know about it?" next="M13" />
		</message>
		<message id="M13" text="[ACTION]Vatishâ€™s expression becomes much more serious.">
			<reply id="R1" text="[SKIP]..." next="M14" />
		</message>
		<message id="M14" text="There is powerful alien technology in your 'prototype' ship.">
			<reply id="R1" text="[SKIP]..." next="M15" />
		</message>
		<message id="M15" text="In fact, it never belonged to the humans in the first place.">
			<reply id="R1" text="[SKIP]..." next="M16" />
		</message>
		<message id="M16" text="But somehow, you got a hold of that technology. And what did you do?">
			<reply id="R1" text="[SKIP]..." next="M17" />
		</message>
		<message id="M17" text="You reverse-engineered it into a super-weapon.">
			<reply id="R1" text="[SKIP]..." next="M18" />
		</message>
		<message id="M18" text="It's a threat to every living being in the universe!">
			<reply id="R1" text="...And the pirates have it now too." next="M19" />
		</message>
		<message id="M19" text="Exactly. They need to be stopped, no matter what.">
			<reply id="R1" text="I'll stop them any way I can." next="M20" />
		</message>
		<message id="M20" text="If you promise us you will destroy it, the collective will assist you.">
			<reply id="R1" text="[SKIP]..." next="M21" />
		</message>
		<message id="M21" text="It's too dangerous to let it fall into pirate hands.">
			<reply id="R1" text="Collective? What are you talking about?" next="M22" />
		</message>
		<message id="M22" text="[ACTION]The human's form vanishes, revealing a glowing orb of plasma in its place.">
			<reply id="R1" text="What the hell are you?" next="M23" />
		</message>
		<message id="M23" text="We are Starwisps. No time to explain, you can ask on the trip if you must.">
			<reply id="R1" text="[SKIP]..." next="M24" />
		</message>
		<message id="M24" text="[ACTION]The creature sends a pulse of energy into the reactor using thin tendrils of plasma.">
			<reply id="R1" text="[SKIP]..." next="M25" />
		</message>
		<message id="M25" text="I called for our ship, they will assist you on your journey.">
			<reply id="R1" text="Why are you willing to help us?" next="M26" />
		</message>
		<message id="M26" text="If that prototype ship isn't destroyed, we may all perish.">
			<reply id="R1" text="Fair enough." next="M27" />
		</message>
		<!-- Skiped Dialogue enters here -->
		<message id="M27" text="[ACTION]Outside of the station there's a bright flash, and a giant crystal ship jumps close.">
			<event id="SPAWN_WispShip" target="PLAYER" />
			<event id="STARWISP_CAPTAIN_BABBLE_ON" target="PLAYER" />
			<event id="WISPSHIP_COMPLETE" target="PLAYER" />
			<reply id="R1" text="[SKIP]..." next="M28" />
		</message>
		<message id="M28" text="The S.S Aurora is waiting. Radio Solaria on the comms when you get to your ship.">
			<reply id="R1" text="[SKIP]..." next="M29" />
		</message>
		<message id="M29" text="[ACTION]The starwisp creature reassumes the illusion of a human.">
			<reply id="R1" text="[SKIP]..." next="M30" />
		</message>
		<message id="M30" text="I'll stay behind and send any other surviving crew your way. Good luck.">
			<reply id="R1" text="Yes, sir! ..lady..thing." />
			<reply id="R2" text="Thanks!" />
		</message>

		<!-- Late players joining aka P.U.C.C conversation -->
		<message id="PUCC1" text="[ACTION]The androgynous-looking person turns to face you.">
			<reply id="R1" text="[SKIP]..." next="PUCC2" />
		</message>
		
		<!-- Main P.U.C.C branch -->
		<message id="PUCC2" text="Oh! Another survivor! The clone replication system managed to recover you as well.">
			<reply id="R1" text="I don't remember anything. What's going on?" next="PUCCStoryRecap1" />
			<reply id="R2" text="I need to join the crew on our ship. (Skip Recap)" next="PUCC3" />			
			<reply id="R3" text="I'm in a hurry. I need to catch up with my ship. (Skip All Dialogue)" next="PUCCSkip1" />
		</message>

		<!-- Skip branch -->
		<message id="PUCCSkip1" text="Okay, I'll have a ship waiting for you outside. It's called the P.U.C.C.">
			<reply id="R1" text="[SKIP]..." next="PUCC21" />
		</message>
		<!-- Skip branch rejoins main P.U.C.C conversation near the end. -->

		
		<!-- Branch recapping the story for new players -->
		<message id="PUCCStoryRecap1" text="The cloning system was damaged, so you didn't recover all of your memories.">
			<reply id="R1" text="[SKIP]..." next="PUCCStoryRecap2" />	
		</message>
		<message id="PUCCStoryRecap2" text="Long story short, pirates attacked this space station and stole a prototype ship.">
			<reply id="R1" text="[SKIP]..." next="PUCCStoryRecap3" />	
		</message>
		<message id="PUCCStoryRecap3" text="Your mission is to hunt down the Prototype ship DF and destroy it.">
			<reply id="R1" text="[SKIP]..." next="PUCCStoryRecap4" />	
		</message>
		<message id="PUCCStoryRecap4" text="The prototype has a super-weapon made using powerful reverse-engineered alien technology.">
			<reply id="R1" text="[SKIP]..." next="PUCCStoryRecap5" />	
		</message>
		<message id="PUCCStoryRecap5" text="We, the Starwisp collective, agreed to help you in your mission.">
			<reply id="R1" text="[SKIP]..." next="PUCCStoryRecap6" />	
		</message>
		<message id="PUCCStoryRecap6" text="The DF prototype is a threat to all life in the universe. It must be destroyed.">
			<reply id="R1" text="[SKIP]..." next="PUCCStoryRecap7" />	
		</message>
		<message id="PUCCStoryRecap7" text="I'm sure your crew can catch you up to speed otherwise...">
			<reply id="R1" text="Then I need to join the crew on our ship." next="PUCC3" />	
		</message>

		<!-- Story recap rejoins main branch -->
		<message id="PUCC3" text="Right... about that. There's no more proper space ships in this sector you can use.">
			<reply id="R1" text="[SKIP]..." next="PUCC4" />
		</message>
		<message id="PUCC4" text="Even if you got one, it could take forever to catch up with your ship.">
			<reply id="R1" text="[SKIP]..." next="PUCC5" />
		</message>
		<message id="PUCC5" text="I did come up with something for you, however!">
			<reply id="R1" text="What was that?" next="PUCC6" />
		</message>
		<message id="PUCC6" text="[ACTION]Vatish gives a grin like a mad scientist.">
			<reply id="R1" text="[SKIP]..." next="PUCC7" />
		</message>
		<message id="PUCC7" text="This space station has a shipyard that makes Prototype Unmanned Construction Crafts.">
			<reply id="R1" text="[SKIP]..." next="PUCC8" />
		</message>
		<message id="PUCC8" text="P.U.C.Cs for short. They weren't meant to be manned, but...">
			<reply id="R1" text="I'm not sure I like where this is going..." next="PUCC9" />
			<reply id="R2" text="Uhh..." next="PUCC9" />
		</message>
		<message id="PUCC9" text="I modified the blueprints, removed some parts and crammed a life support unit inside.">
			<reply id="R1" text="[SKIP]..." next="PUCC10" />
		</message>
		<message id="PUCC10" text="It's a tight fit, but it's space worthy and very fast, though it lacks a shield generator.">
			<reply id="R1" text="How would that possibly catch up with our ship?" next="PUCC11" />
		</message>
		<message id="PUCC11" text="Well... I've been tinkering, and modified a FTL engine to fit inside it.">
			<reply id="R1" text="[SKIP]..." next="PUCC12" />
		</message>
		<message id="PUCC12" text="I took out all the... 'unnecessary' parts, but it's still quite weak on such a tiny ship.">
			<reply id="R1" text="[SKIP]..." next="PUCC13" />
		</message>
		<message id="PUCC13" text="So I hooked it up to the station and super-charged it with the reactor!">
			<reply id="R1" text="That can't be safe..." next="PUCC14" />
		</message>
		<message id="PUCC14" text="Well... it only needs to work once.">
			<reply id="R1" text="[SKIP]..." next="PUCC15" />
		</message>
		<message id="PUCC15" text="We attached a beacon to your ship so the Aurora can follow it.">
			<reply id="R1" text="[SKIP]..." next="PUCC16" />
		</message>
		<message id="PUCC16" text="I'll program it into the FTL system and the autopilot will follow the ship automatically.">
			<reply id="R1" text="[SKIP]..." next="PUCC17" />
		</message>
		<message id="PUCC17" text="The P.U.C.C is so small it can ride the FTL wake your ship makes when it jumps.">
			<reply id="R1" text="[SKIP]..." next="PUCC18" />
		</message>
		<message id="PUCC18" text="It can also be handy in a dogfight. It's fast and can damage the hull or systems quickly.">
			<reply id="R1" text="[SKIP]..." next="PUCC19" />
		</message>
		<message id="PUCC19" text="But it doesn't have any shields, so make sure to dodge frequently.">
			<reply id="R1" text="Okay. You really think this will work?" next="PUCC20" />
		</message>
		<message id="PUCC20" text="I'm... pretty sure it will work. The P.U.C.C is outside.">
			<reply id="R1" text="[SKIP]..." next="PUCC21" />
		</message>

		<!-- P.U.C.C conversation skip lands here -->
		<message id="PUCC21" text="Contact me on the comms when you're ready and I'll install the FTL unit.">
			<event id="SPAWN_PUCC" target="PLAYER" />
			<!-- Spawns the P.U.C.C outside -->
			<reply id="R1" text="Will do." />
		</message>

		<!-- Ready to warp the P.U.C.C conversation -->
		<message id="WARP_PUCC1" text="Okay, make sure you're INSIDE the ship and have already claimed it.">
			<reply id="R1" text="Wait! I'll get in the ship and claim it." />
			<reply id="R2" text="Yes, ready to go!" next="WARP_PUCC2">
				<condition type="SECTOR_SHIPS_PRESENT">
					<condition_param target_player="1"></condition_param>
					<condition_param ship_name="P.U.C.C"></condition_param>
					<condition_param crew="[PLAYER_NAME]"></condition_param>
					<condition_param qty="1"></condition_param>
				</condition>
				<condition type="SHIP_SYSTEM_ABSENT">
					<condition_param system_type="HYPERDRIVE"></condition_param>
					<condition_param system_model="PUCC_HyperDrive"></condition_param>
					<condition_param target_player="1"></condition_param>
					<condition_param all_mob_ships="0"></condition_param>
					<condition_param active_system="0"></condition_param>
					<condition_param qty="1"></condition_param>
				</condition>
				<condition type="SERVER_VARIABLE_PRESENT" var_name="PUCC_BEACON_SECTOR_LOC" var_value="1" any_value="1" />
			</reply>
			<reply id="R3" text="Fire away! (Skip Dialogue)" next="WARP_PUCC5">
			<!-- Skip the few lines of dialogue -->
				<condition type="SECTOR_SHIPS_PRESENT">
					<condition_param target_player="1"></condition_param>
					<condition_param ship_name="P.U.C.C"></condition_param>
					<condition_param crew="[PLAYER_NAME]"></condition_param>
					<condition_param qty="1"></condition_param>
				</condition>
				<condition type="SHIP_SYSTEM_ABSENT">
					<condition_param system_type="HYPERDRIVE"></condition_param>
					<condition_param system_model="PUCC_HyperDrive"></condition_param>
					<condition_param target_player="1"></condition_param>
					<condition_param all_mob_ships="0"></condition_param>
					<condition_param active_system="0"></condition_param>
					<condition_param qty="1"></condition_param>
				</condition>
				<condition type="SERVER_VARIABLE_PRESENT" var_name="PUCC_BEACON_SECTOR_LOC" var_value="1" any_value="1" />
			</reply>
		</message>

		<message id="WARP_PUCC2" text="The modified FTL engine is ready, and will engage the moment I install it." next="WARP_PUCC3">
			<reply id="R1" text="[SKIP]..." next="WARP_PUCC3" />
		</message>
		<message id="WARP_PUCC3" text="By the way... the ship has a built-in fire suppression and repair system.">
			<reply id="R1" text="Oh nice!" next="WARP_PUCC4" />
		</message>
		<message id="WARP_PUCC4" text="It'll come in handy if it bursts into flames from the FTL engine.">
			<reply id="R1" text="Wait, what?" next="WARP_PUCC5" />
			<reply id="R2" text="Oh, great..." next="WARP_PUCC5" />
		</message>
		<message id="WARP_PUCC5" text="Bon voyage!">
			<reply id="R1" text="[SKIP]..." />
			<event id="PUCC_Follow_Beacon" target="PLAYER" />
		</message>


		<message id="BeaconLost1" text="...Strange. I can't find any trace of the beacon we put on your ship.">
			<reply id="R1" text="[SKIP]..." next="BeaconLost2" />
		</message>
		<message id="BeaconLost2" text="Without it, I don't know where to send you.">
			<reply id="R1" text="[SKIP]..." next="BeaconLost3" />
		</message>
		<message id="BeaconLost3" text="I'm afraid I can't help you without a beacon to guide us.">
			<reply id="R1" text="[SKIP]..." next="BeaconLost4" />
		</message>
		<message id="BeaconLost4" text="(If the campaign ship was destroyed, it's Game Over. Reset the server.)">
			<reply id="R1" text="[SKIP]..." />
		</message>
	</dialogue>

	<dialogue name="Solaria">
		<start>M0</start>
		<message id="M0" text="[ACTION]You see a white wispy creature surrounded by screens.">
			<reply id="R01" text="...Hello?" next="M1">
				<condition type="SERVER_VARIABLE_ABSENT" var_name="SOLARIA_CONVO_1" var_value="1" />
			</reply>
			<reply id="R02" text="Please stop following or guarding the VAR(WISPSHIP_GUARD_ID) for now." next="StopGuard1">
				<condition type="SERVER_VARIABLE_PRESENT" var_name="SOLARIA_CONVO_1" var_value="1" />
				<condition type="SERVER_VARIABLE_PRESENT" var_name="WISPSHIP_GUARD_ID" var_value="1" any_value="1" />
				<condition type="SECTOR_SHIPS_PRESENT" target_player="1" crew="[PLAYER_NAME]" qty="1" />
			</reply>
			<reply id="R03" text="Guard our ship please." next="StartGuard1">
				<condition type="SERVER_VARIABLE_PRESENT" var_name="SOLARIA_CONVO_1" var_value="1" />
				<condition type="SERVER_VARIABLE_ABSENT" var_name="WISPSHIP_GUARD_ID" var_value="1" any_value="1" />
				<condition type="SECTOR_SHIPS_PRESENT" target_player="1" crew="[PLAYER_NAME]" qty="1" />
			</reply>
			<reply id="R04" text="About your cargo..." next="Cargo1" any_condition="1">
				<condition type="SHIP_CARGO_PRESENT" cargo_type="SHIP_SYSTEM" system_type="LASER_WEAPONS" target_player="0" qty="1" />
				<condition type="SHIP_CARGO_PRESENT" cargo_type="SHIP_SYSTEM" system_type="BEAM_WEAPONS" target_player="0" qty="1" />
				<condition type="SHIP_CARGO_PRESENT" cargo_type="SHIP_SYSTEM" system_type="MISSILE_WEAPONS" target_player="0" qty="1" />
				<condition type="SHIP_CARGO_PRESENT" cargo_type="SHIP_SYSTEM" system_type="SHIELDS" target_player="0" qty="1" />
				<condition type="SHIP_CARGO_PRESENT" cargo_type="SHIP_SYSTEM" system_type="REACTOR" target_player="0" qty="1" />
				<condition type="SHIP_CARGO_PRESENT" cargo_type="SHIP_SYSTEM" system_type="OXYGEN" target_player="0" qty="1" />
				<condition type="SHIP_CARGO_PRESENT" cargo_type="SHIP_SYSTEM" system_type="PILOTING" target_player="0" qty="1" />
				<condition type="SHIP_CARGO_PRESENT" cargo_type="SHIP_SYSTEM" system_type="TELEPORT" target_player="0" qty="1" />
				<condition type="SHIP_CARGO_PRESENT" cargo_type="SHIP_SYSTEM" system_type="CAPACITOR" target_player="0" qty="1" />
				<condition type="SHIP_CARGO_PRESENT" cargo_type="SHIP_SYSTEM" system_type="DOOR_CONTROL" target_player="0" qty="1" />
				<condition type="SHIP_CARGO_PRESENT" cargo_type="SHIP_SYSTEM" system_type="ENGINES" target_player="0" qty="1" />
				<condition type="SHIP_CARGO_PRESENT" cargo_type="SHIP_SYSTEM" system_type="HYPERDRIVE" target_player="0" qty="1" />
				<condition type="SHIP_CARGO_PRESENT" cargo_type="SHIP_SYSTEM" system_type="MEDICAL" target_player="0" qty="1" />
				<condition type="SHIP_CARGO_PRESENT" cargo_type="SHIP_SYSTEM" system_type="SENSORS" target_player="0" qty="1" />
				<condition type="SHIP_CARGO_PRESENT" cargo_type="SHIP_SYSTEM" system_type="REPAIR" target_player="0" qty="1" />
				<condition type="SHIP_CARGO_PRESENT" cargo_type="SHIP_SYSTEM" system_type="SYSTEMS_SHOP" target_player="0" qty="1" />
